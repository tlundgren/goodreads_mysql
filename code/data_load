-- Loads data from a GoodReads library export as csz file into table list_raw
LOAD DATA LOCAL INFILE '/media/sf_Shared_folder/goodreads.csv' INTO TABLE `goodreads`.`list_raw` FIELDS TERMINATED BY ',' ENCLOSED BY '"' LINES TERMINATED BY '\n' IGNORE 1 ROWS (`book_id`,`title`,`author`,`author_lf`,`author_additional`,`isbn`,`isbn13`,`rating_my`,`rating_average`,`publisher`,`binding`,`page_count`,`publication_year`,`publication_year_original`,`read_date`,`added_date`,`bookshelves`,`bookshelves_positions`,`bookshelve_exclusive`,`review_my`,`spoiler`,`notes_private`,`read_count`,`recommended_for`,`recommended_by`,`owned_copies`,`original_purchase_date`,`original_purchase_location`,`condition`,`condition_description`,`bcid`);
